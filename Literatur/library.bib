doi = {10.1002/cite.201200034},
file = {:C$\backslash$:/Users/Meret Feldkamper/Documents/Diplomarbeit/Diplomarbeit/Literatur-sortiert/Urbas et al. - Modularisierung und Prozessführung.pdf:pdf},
