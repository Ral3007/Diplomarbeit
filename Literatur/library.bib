@article{Miller2005,
abstract = {The authors begin by contrasting the 'adaptive' automation systems prevalent in Augmented Cognition research and development with 'adaptable' systems that provide the same range of modifications and behaviours but place control over those adaptations in the hands of the human operator. While there are many obvious reasons to seek adaptive systems, there are also some less well-known consequences to high levels of automation control in human-machine systems. The authors review the literature on human interaction with high levels of automation in complex and critical work environments to illustrate various problematic effects including loss of situation awareness, poorly tuned trust, skill degradation, unbalanced mental workload, lack of user acceptance and, most importantly, poorer overall human + machine system performance and decision making. They illustrate how these effects may be mitigated by maintaining a more adaptable approach to automation use and suggest an approach to flexible adaptability, the Playbook{\^{a}}„¢ delegation approach, which seeks to provide the best of both approaches.},
author = {Miller, C a and Funk, H B and Goldman, R P and Meisner, J and Wu, P},
