issn = {10715819},
journal = {International Journal of Human Computer Studies},
keywords = {Cognitive cooperation,Communication,Cooperation assessment,Cooperation support,Cooperative machine,Human-human cooperation,Human-machine cooperation},
number = {4},
pages = {509--540},
pmid = {10554843},
title = {{Towards a cognitive approach to human-machine cooperation in dynamic situations}},
volume = {54},
year = {2001}
}
