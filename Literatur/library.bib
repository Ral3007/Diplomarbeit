isbn = {0805857826 (ISBN); 9780805857825 (ISBN)},
journal = {Computer Supported Collaborative Learning 2005: The Next 10 Years - Proceedings of the International Conference on Computer Supported Collaborative Learning 2005, CSCL 2005},
keywords = {Collaboration,Conjoint participation.,Interaction,Quasi-synchronous,Quasi-synchronous online chats,Reading,Telephone systems},
pages = {753--757},
primaryClass = {math},
title = {{Texts-in-interaction: Collaborative problem-solving in quasi-synchronous computer-mediated communication}},
